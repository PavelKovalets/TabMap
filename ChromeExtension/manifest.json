{
  "manifest_version": 2,

  "name": "Tab Maps",
  "description": "This extension adds functionality of Yandex.Maps service to tab.by website.",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "http://tab.by/"
  ]
}